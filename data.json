{
    "rules": 
    { 
        "users": 
        [
            {
                "user_id": "$uid",
                "activities":
                [
                    {
                        "activity_name": "$activity_name",
                        "years":
                        [
                            {
                                "year": "$year",
                                "months":
                                [
                                    {
                                        "month": "$month",
                                        "days":
                                        [
                                            {
                                                "day": "$day",
                                                "entries":
                                                [
                                                    {
                                                        "count": "$count",
                                                        "time_elapsed": "$time_elapsed",
                                                        "time_set": "$time_set"
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}